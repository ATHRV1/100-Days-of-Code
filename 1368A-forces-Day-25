#include <iostream>
using namespace std;

int main(int argc, char const *argv[])
{
    int n;
    cin>>n;
    

    for (int i = 0; i < n; i++)
    {
        int count =0;
        int a,b,n;
        cin>>a>>b>>n;

        while (a<=n && b<=n)
        {
            if (a<b)
            {
                a+=b;
                
            }else{
                b+=a;
                
            }
            count++;
        }
        cout<<count<<endl;
    }
    
    return 0;
}
