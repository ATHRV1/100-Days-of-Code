#include <iostream>
using namespace std;

int main(int argc, char const *argv[])
{

    int t;
    cin >> t;

    while (t--)
    {

        int a, b;
        cin >> a >> b;

        if (a == b)
        {
            cout << "YES" << endl;
        }
        else if(a==2 && b==3){
            cout<<"YES"<<endl;
        }
        else if (a <= 3 && b > a)
        {
            cout << "NO" << endl;
        }
        else
        {
            cout << "YES" << endl;
        }
    }

    return 0;
}
