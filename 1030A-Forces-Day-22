#include <iostream>
using namespace std;

int main(int argc, char const *argv[])
{
    int count =0;
    int n;
    cin>>n;
    int arr[n];

    for (int i = 0; i < n; i++)
    {
        int a;
        cin>>a;
        arr[i] = a;
    }
    for (int i = 0; i < n; i++)
    {
        if (arr[i] == 1)
        {
            count++;
        }
        
        
    }
    if (count>0)
    {
        cout<<"HARD"<<endl;
    }else{
        cout<<"EASY"<<endl;
    }
    
    
    
    
    return 0;
}
