#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        long long a;
        cin >> a;

        if ((a - 14) % 14 >= 1 && (a - 14) % 14 <= 6 && a >= 15) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
