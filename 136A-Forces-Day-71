#include <iostream>
#include <vector>
using namespace std;

int main(int argc, char const *argv[])
{
    int n;
    cin>>n;
    vector <int> arr(n);

    for (int i = 0; i < n; i++)
    {
        int a;
        cin>>a;
        arr[i]=a;
    }
    for (int j = 1; j <=n; j++)
    {
        for (int i = 0; i < n; i++)
        {
            if (arr[i]==j)
            {
                cout<<i+1<<" ";
            }
            
        }
        
    }
    
    
    return 0;
}
