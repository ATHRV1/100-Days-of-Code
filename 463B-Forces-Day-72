#include <iostream>
#include<algorithm>
#include <vector>
using namespace std;

int main(int argc, char const *argv[])
{
    long long n;
    cin >> n;
    vector<int> arr(n);

    for (int i = 0; i < n; i++)
    {
        int a;
        cin >> a;
        arr[i] = a;
    }
    sort(arr.begin(),arr.end());
    cout<<arr[n-1];
    

    return 0;
}
