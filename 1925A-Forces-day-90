#include <iostream>
#include <string>
using namespace std;

int main(int argc, char const *argv[])
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin>>t;
    for (int i = 0; i < t; i++)
    {
        
    
    
    char arr[26]={'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
    int a,b;
    cin>>a>>b;
    for (int i = 0; i < a; i++)
    {
       for (int i = 0; i < b; i++)
       {
        cout<<arr[i];
       }
        
    }
    cout<<endl;
    }
    

    return 0;
}
