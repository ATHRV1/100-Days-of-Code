#include <iostream>
using namespace std;

int main(int argc, char const *argv[])
{
    int t;
    cin >> t;

    while (t--)
    {

        int a, b;
        cin >> a >> b;

        if (a % b == 0)
        {
            cout << "0" << endl;
        }
        else if (a > b)
        {
            cout << b-(a % b) << endl;
        }
        else if (b > a)
        {
            cout << b - a << endl;
        }
    }
    return 0;
}
